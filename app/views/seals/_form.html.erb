<%= simple_form_for([@city, @seal]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id, :value => params[:project_id] %>
    <%= f.hidden_field :city_id, :value => @city.id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
